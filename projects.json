    {
      "name": "Vim",
      "url": "https://github.com/vim/vim.git"
    },
    {
      "name": "Neovim",
      "url": "https://github.com/neovim/neovim.git"
    },
    {
      "name": "Bitcoin",
      "url": "https://github.com/bitcoin/bitcoin.git",
      "configure_command": "./autogen.sh && ./configure --disable-wallet --disable-static --disable-tests"
